@model pb_category.Models.CalculateViewModels
<div class="accordion">
    @for (int i = 1; i <= 8; i++)
    {
        <div class="accordion-section">
            <a class="accordion-section-title" href="#accordion-@i">
                Шаг @i
                @if ((int)ViewBag.Step > i)
                {
                    { Html.RenderPartial("StepA" + @i + "_H"); }
                }
            </a>
            @if ((int)ViewBag.Step >= i)
            {
                <div id="accordion-@i" class="accordion-section-content">
                    @{ Html.RenderPartial("StepA" + @i); }
                    @if ((int)ViewBag.Step == i)
                    {
                        <input type="submit" class="btn center" value="Перейти к следующему шагу" id="Answer" />
                    }
                </div>
            }
        </div>
    }
</div>
